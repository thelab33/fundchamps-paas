        const target = +el.dataset.target;
        let count = 0;
        const increment = Math.max(1, Math.ceil(target / 50));
        function step() {
          count += increment;
          if (count >= target) {
            el.textContent = target;
          } else {
            el.textContent = count;
            requestAnimationFrame(step);
          }
        }
