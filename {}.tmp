        aria-describedby="starforge-email-error"
        required
      />
      <div id="starforge-email-error"></div>
      <button type="submit">
        {{ org_newsletter_button if org_newsletter_button is defined else
        "Subscribe" }}
      </button>
      <div id="starforge-newsletter-thankyou"></div>
